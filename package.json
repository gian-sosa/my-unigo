{
  "name": "tailwindcss4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:secure": "npm run security:check && vite build --mode production",
    "clean:debug": "echo 'Debug files cleanup - handled by .gitignore'",
    "security:check": "npm run audit:deps && npm run audit:licenses && npm run lint:security",
    "audit:deps": "npm audit --audit-level=moderate",
    "audit:licenses": "npm audit --audit-level=low",
    "audit:fix": "npm audit fix",
    "lint": "eslint .",
    "lint:security": "eslint . --config eslint.security.config.js",
    "preview": "vite preview",
    "deps:outdated": "npm outdated",
    "deps:update": "npm update"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@tailwindcss/vite": "^4.1.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "styled-components": "^6.1.19",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.0"
  }
}
